{
  "expo": {
    "name": "redro",
    "slug": "redro",
    "scheme": "template",
    "version": "0.0.2",
    "owner": "redro",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "1233bed6-82c7-4703-a4cf-9732a305a7c1"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc",
      "bundleIdentifier": "com.order.app"
    },
    "android": {
      "versionCode": 2,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/splash.png"
    }
  }
}
